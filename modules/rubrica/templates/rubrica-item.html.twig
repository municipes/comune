<blockquote>
	{% for item in content.items %}
		{% if item.type == 'persona' %}
			{% import "@rubrica/persona.html.twig" as personaFields %}
			{{ personaFields.persona(item.value) }}
		{% else %}
      {% import "@rubrica/uo.html.twig" as uoFields %}
			{{ uoFields.uo(item.value) }}
		{% endif %}
		<hr>
	{% endfor %}
</blockquote>
