{
  "name": "municipes/comune",
  "description": "The installation profile for municipality by Silfi.",
  "type": "drupal-profile",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "Maurizio Cavalletti",
      "homepage": "https://www.drupal.org/u/maurizio_akabit",
      "role": "Maintainer"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "php": "^8.1",
    "composer/installers": "^2.0",
    "cweagans/composer-patches": "^1.6.0",
    "municipes/silfi_triplette": "^1.0",
    "municipes/privatum_documentum": "^1.x-dev",
    "municipes/libellus": "^1.x-dev",
    "municipes/nuntium": "^1.x-dev",
    "municipes/scaenographia": "^1.0"
  },
  "support": {
    "docs": "https://drupal.org/project/comune_profile"
  },
  "replace": {
    "drupal/comune": "self.version"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "cweagans/composer-patches": true
    }
  }
}
